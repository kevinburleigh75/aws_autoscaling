[Unit]
Description=Kevin's Response/Calc Creator Service
After=network.target

[Service]
Type=simple
User=ubuntu
WorkingDirectory=/home/ubuntu
ExecStart=/bin/bash /home/ubuntu/aws_autoscaling/services/run_response_calc_creator.sh
Restart=on-abort
KillSignal=SIGINT
TimeoutStopSec=5

[Install]
WantedBy=multi-user.target
